<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>RedirectFrom</servlet-name>
        <servlet-class>test.RedirectFrom</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RedirectTo</servlet-name>
        <servlet-class>test.RedirectTo</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RedirectFrom</servlet-name>
        <url-pattern>/redirectFrom</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RedirectTo</servlet-name>
        <url-pattern>/redirectTo</url-pattern>
    </servlet-mapping>

    <session-config>
        <tracking-mode>URL</tracking-mode>
    </session-config>
</web-app>


